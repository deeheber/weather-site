{
  "name": "weather-site",
  "version": "0.1.0",
  "bin": {
    "weather-site": "bin/weather-site.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "npm run build && jest",
    "cdk": "cdk",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "clean": "rm -rf dist",
    "deploy": "npm run clean && npm run build && npm run cdk deploy -- --all",
    "destroy": "npm run cdk destroy -- --all"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "20.9.1",
    "aws-cdk": "2.110.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.5",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.451.0",
    "aws-cdk-lib": "2.110.0",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  }
}
