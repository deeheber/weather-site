{
  "name": "weather-site",
  "version": "0.1.0",
  "bin": {
    "weather-site": "bin/weather-site.js"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "clean": "rm -rf dist",
    "deploy": "npm run clean && npm run build && npm run cdk deploy -- --all",
    "deploy:ci": "npm run clean && npm run build && npm run cdk deploy -- --all --require-approval never",
    "destroy": "npm run cdk destroy -- --all",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:ci": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint --fix",
    "lint:ci": "eslint --quiet",
    "synth": "npm run build && npm run cdk synth",
    "synth:quiet": "npm run build && npm run cdk synth -- -q",
    "test": "npm run build && jest",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/jest": "^29.5.14",
    "@types/node": "22.15.30",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "aws-cdk": "^2.1018.0",
    "dotenv": "^16.5.0",
    "esbuild": "^0.25.5",
    "eslint": "^9.28.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.825.0",
    "@aws-sdk/client-secrets-manager": "^3.825.0",
    "aws-cdk-lib": "2.200.1",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  }
}
