{
  "name": "weather-site",
  "version": "0.1.0",
  "bin": {
    "weather-site": "bin/weather-site.js"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "clean": "rm -rf dist",
    "deploy": "npm run clean && npm run build && npm run cdk deploy -- --all",
    "destroy": "npm run cdk destroy -- --all",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint --fix \"**/*.{ts,mjs,tsx,json,md}\" --ignore-pattern .gitignore --no-error-on-unmatched-pattern",
    "test": "npm run build && jest",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@types/jest": "^29.5.14",
    "@types/node": "22.13.0",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "aws-cdk": "^2.177.0",
    "dotenv": "^16.4.5",
    "esbuild": "^0.24.2",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.740.0",
    "aws-cdk-lib": "2.177.0",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  }
}
