{
  "name": "weather-site",
  "version": "0.1.0",
  "bin": {
    "weather-site": "bin/weather-site.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "clean": "rm -rf dist",
    "deploy": "npm run clean && npm run build && npm run cdk deploy -a WeatherSiteStack",
    "destroy": "npm run cdk destroy -a WeatherSiteStack"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "18.11.9",
    "aws-cdk": "2.88.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.18.15",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.298.0",
    "aws-cdk-lib": "2.88.0",
    "constructs": "^10.1.289",
    "source-map-support": "^0.5.21"
  }
}
